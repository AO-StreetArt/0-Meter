<Configuration>
  <Behavior>
    <Multi_Message>True</Multi_Message>
    <Include_CSV>True</Include_CSV>
  </Behavior>
  <Message>
    <Message_Location>ci/msg/test.txt</Message_Location>
    <Message_Extension>txt</Message_Extension>
    <CSV_Location>ci/csv/test.csv</CSV_Location>
    <Variable_Start_Character>$</Variable_Start_Character>
    <Variable_End_Character>@</Variable_End_Character>
  </Message>
  <ZeroMQ>
    <Outbound_Connection>tcp://localhost:5555</Outbound_Connection>
    <Outbound_Connection_Type>REQ</Outbound_Connection_Type>
  </ZeroMQ>
  <Logging>
    <Log_File>csv_test.log</Log_File>
    <Log_Level>Debug</Log_Level>
  </Logging>
</Configuration>
