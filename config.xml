<Configuration>
  <Behavior>
    <Single_Message>False</Single_Message>
    <Multi_Message>True</Multi_Message>
    <Include_CSV>True</Include_CSV>
    <Span_Over_Interval>True</Span_Over_Interval>
  </Behavior>
  <Message>
    <Message_Location>helloworld.txt</Message_Location>
    <Message_Folder_Location>messages</Message_Folder_Location>
    <Message_Extension>txt</Message_Extension>
    <Interval>30.0</Interval>
    <CSV_Location>test.csv</CSV_Location>
    <Variable_Start_Character>$</Variable_Start_Character>
    <Variable_End_Character>@</Variable_End_Character>
  </Message>
  <ZeroMQ>
    <Outbound_Connection>tcp://localhost:5555</Outbound_Connection>
    <Outbound_Connection_Type>REQ</Outbound_Connection_Type>
  </ZeroMQ>
  <Logging>
    <Log_File>helloworld.log</Log_File>
    <Log_Level>Debug</Log_Level>
  </Logging>
</Configuration>
