<Configuration>
  <Behavior>
    <Single_Message>False</Single_Message>
    <Multi_Message>True</Multi_Message>
    <Include_CSV>True</Include_CSV>
    <Span_Over_Interval>False</Span_Over_Interval>
  </Behavior>
  <Message>
    <Message_Location>scene.json</Message_Location>
    <Message_Folder_Location>obj_msg</Message_Folder_Location>
    <Message_Extension>json</Message_Extension>
    <Interval>60.0</Interval>
    <CSV_Location>scene_schedule.csv</CSV_Location>
    <Variable_Start_Character>$</Variable_Start_Character>
    <Variable_End_Character>@</Variable_End_Character>
  </Message>
  <ZeroMQ>
    <Outbound_Connection>tcp://localhost:5556</Outbound_Connection>
    <Outbound_Connection_Type>REQ</Outbound_Connection_Type>
  </ZeroMQ>
  <Logging>
    <Log_File>scene_sched.log</Log_File>
    <Log_Level>Debug</Log_Level>
  </Logging>
</Configuration>
